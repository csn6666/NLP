    "### 用wiki+新闻，训练个新的词向量"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 271,
   "metadata": {},
   "outputs": [],
   "source": [
    "import pickle\n",
    "train_data = []\n",
    "for filename in os.listdir('/Users/sinong/data/train_data/'):\n",
    "    if filename == '.DS_Store': continue\n",
    "    with open('/Users/sinong/data/train_data/' + str(filename), 'rb') as f:\n",
    "        train_data.extend(pickle.load(f)) \n",
    "with open('./news.txt', 'r') as f:\n",
    "    for line in f:\n",
    "        words = jieba.cut(''.join(re.findall(r'\\w+',line)))\n",
    "        train_data.extend([word for word in words])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 277,
   "metadata": {},
   "outputs": [],
   "source": [
    "with open('/Users/sinong/data/wiki-news-sentences-cut.txt', 'w') as f:\n",
    "    for line in train_data:\n",
    "        f.write(' '.join(line) + '\\n')"
   ]
